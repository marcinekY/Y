<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:i="urn:import:pl.cms.system.client.ui"
	xmlns:help="urn:import:pl.cms.system.client.uihelpers">
	<ui:style>
		.info-content {
			width: 100%;
			height: 500px;
			padding: 10px 25px;
			background: #cccccc;
		}
	</ui:style>


	<g:AbsolutePanel ui:field="panel">
		<g:HTMLPanel ui:field="menuBarPanel" styleName="Y-system-menuBarBox" />
		<help:OneWidgetHTMLPanel styleName="Y-system-contentPanel"
			ui:field='contentPanel' />
	</g:AbsolutePanel>

</ui:UiBinder>